<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" fillHeight="false" maxWidth="1.7976931348623157E308" scaleShape="false" spacing="8.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hypersocket.client.gui.jfx.Update">
   <children>
      <HBox alignment="CENTER" spacing="8.0">
         <children>
            <ProgressBar fx:id="progress" maxWidth="1.7976931348623157E308" prefWidth="200.0" progress="0.0" HBox.hgrow="ALWAYS" />
            <Label fx:id="message" maxWidth="1.7976931348623157E308" text="%updating" wrapText="true" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
   <styleClass>
      <String fx:value="background" />
      <String fx:value="transparent" />
   </styleClass>
</HBox>
